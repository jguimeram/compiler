function fib($n) {
    if ($n == 0) {
        return 0;
    }
    if ($n == 1) {
        return 1;
    }
    return fib($n - 1) + fib($n - 2);
}

// Compute Fibonacci numbers 0â€“9
$i = 0;
while ($i < 10) {
    $value = fib($i);
    print($value);
    $i = $i + 1;
}

